<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ccffff">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:id="@+id/form_list_time"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:text="2021年03月05日 21:37"></TextView>
        <TextView android:id="@+id/form_list_wendu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="36.2"
            android:layout_marginLeft="3dp"></TextView>
        <TextView android:id="@+id/form_list_stuation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="良好"
            android:layout_marginLeft="3dp"></TextView>
        <TextView android:id="@+id/form_list_address"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:text="中国河北省衡水市武邑县武邑镇祥云小区"></TextView>
        <TextView android:id="@+id/form_list_special"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:text="过去或现居在中高风险地区"></TextView>

    </LinearLayout>

</RelativeLayout>